{
  "manifest_version": 2,
  "name": "Remove ugly teachers",
  "version": "1.0.2",
  "description": "A browser plugin to remove the teacher caricatures on MinUddannelse",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.minuddannelse.net/*"],
      "js": ["index.js"]
    }
  ]
}
